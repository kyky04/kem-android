<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <android.support.design.widget.AppBarLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@android:color/transparent"
            >

            <android.support.v7.widget.Toolbar
                android:id="@+id/toolbar"
                android:layout_width="match_parent"
                android:layout_height="?attr/actionBarSize"
                app:contentInsetStartWithNavigation="0dp"
                android:padding="@dimen/spacing_medium">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:orientation="horizontal">

                   <TextView
                       fontPath="fonts/lato_regular.ttf"
                       android:textSize="25dp"
                       android:gravity="center"
                       android:padding="5dp"
                       android:background="@drawable/txt_area"
                       android:layout_width="150dp"
                       android:layout_height="wrap_content"
                       android:textStyle="bold"
                       android:textColor="@drawable/ic_add_black"
                       android:text="KEM"/>
                </LinearLayout>

            </android.support.v7.widget.Toolbar>

            <!--<TextView-->
                <!--android:id="@+id/tv_welcome"-->
                <!--fontPath="fonts/lato_regular.ttf"-->
                <!--android:layout_width="match_parent"-->
                <!--android:layout_height="wrap_content"-->
                <!--android:gravity="center"-->
                <!--android:textSize="16dp"-->
                <!--android:padding="@dimen/spacing_medium"-->
                <!--android:text="Kemampuan Efektif Membaca"-->

                <!--tools:ignore="MissingPrefix" />-->


        </android.support.design.widget.AppBarLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:padding="20dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal"
                android:background="@drawable/ic_menu1_background"
                android:id="@+id/btn_latihan">
                <LinearLayout
                    android:layout_weight="1"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center|left"
                    android:padding="30dp">

                    <TextView
                        fontPath="fonts/lato_regular.ttf"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Latihan"
                        android:textSize="30sp"
                        tools:ignore="MissingPrefix" />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:layout_below="@+id/rapat_k3"
                    android:layout_weight="2"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:padding="@dimen/spacing_medium">

                    <ImageView

                        android:id="@+id/rapat_k3"
                        android:layout_width="match_parent"
                        android:layout_height="100dp"
                        android:padding="@dimen/spacing_large"
                        android:src="@drawable/ic_latihan" />

                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_marginTop="10dp"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal"
                android:background="@drawable/ic_menu2_background"
                android:id="@+id/btn_petunjuk">
                <LinearLayout
                    android:layout_weight="1"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center|left"
                    android:padding="30dp">

                    <TextView
                    fontPath="fonts/lato_regular.ttf"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Petunjuk"
                    android:textSize="30sp"
                    tools:ignore="MissingPrefix" />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:layout_below="@+id/rapat_k3"
                    android:layout_weight="2"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:padding="@dimen/spacing_medium">

                    <ImageView
                    android:id="@+id/img_inspeksi"
                    android:layout_width="match_parent"
                    android:layout_height="100dp"
                    android:padding="@dimen/spacing_large"
                    android:src="@drawable/ic_petunjuk1" />

                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_marginTop="10dp"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal"
                android:background="@drawable/ic_menu4_background"
                android:id="@+id/btn_info">
                <LinearLayout
                    android:layout_weight="1"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center|left"
                    android:padding="30dp">

                    <TextView
                        fontPath="fonts/lato_regular.ttf"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Info"
                        android:textSize="30sp"
                        tools:ignore="MissingPrefix" />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:layout_below="@+id/rapat_k3"
                    android:layout_weight="2"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:padding="@dimen/spacing_medium">

                    <ImageView
                        android:id="@+id/img_talk"
                        android:layout_width="match_parent"
                        android:layout_height="100dp"
                        android:padding="@dimen/spacing_large"
                        android:src="@drawable/ic_info" />

                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_marginTop="10dp"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal"
                android:background="@drawable/ic_menu3_background"
                android:id="@+id/btn_tentang">
                <LinearLayout
                    android:layout_weight="1"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center|left"
                    android:padding="30dp">

                    <TextView
                        fontPath="fonts/lato_regular.ttf"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Tentang"
                        android:textSize="30sp"
                        tools:ignore="MissingPrefix" />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:layout_below="@+id/rapat_k3"
                    android:layout_weight="2"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:padding="@dimen/spacing_medium">

                    <ImageView
                        android:id="@+id/img_meeting"
                        android:layout_width="match_parent"
                        android:layout_height="100dp"
                        android:padding="@dimen/spacing_large"
                        android:src="@drawable/id_grafik" />

                </LinearLayout>
            </LinearLayout>
        </LinearLayout>


